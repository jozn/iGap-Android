<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/white"
    android:layoutDirection="ltr"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp60"
        android:background="@color/light_green"
        android:layoutDirection="ltr"
        android:weightSum="10" >

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@color/light_green"
            android:gravity="center"
            android:layoutDirection="ltr"
            android:orientation="vertical" >

            <Button
                android:id="@+id/btn_privacy_back"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@null"
                android:layoutDirection="ltr"
                android:text="@string/fa_angle_left"
                android:textColor="@color/white"
                android:textSize="@dimen/drawer_font_size" />
        </LinearLayout>

        <TextView
            android:id="@+id/txt_str_privacy_security"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="9"
            android:gravity="left|center"
            android:paddingLeft="20dp"
            android:singleLine="true"
            android:text="@string/privacy_security_en"
            android:textColor="@color/white"
            android:textSize="@dimen/xlarg_font" />
    </LinearLayout>

    <ScrollView
        android:id="@+id/scrollView1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        android:layoutDirection="ltr" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layoutDirection="ltr"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp180"
                android:layoutDirection="ltr"
                android:weightSum="10" >

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:layoutDirection="ltr"
                    android:orientation="vertical" >
                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="9"
                    android:layoutDirection="ltr"
                    android:orientation="vertical"
                    android:weightSum="1.2" >

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight=".5"
                        android:layoutDirection="ltr"
                        android:orientation="vertical" >

                        <TextView
                            android:id="@+id/txt_str_privacy"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:gravity="left|center"
                            android:paddingTop="5dp"
                            android:singleLine="true"
                            android:text="@string/privacy_en"
                            android:textColor="@color/light_green"
                            android:textSize="@dimen/xlarg_font" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight=".2"
                        android:layoutDirection="ltr"
                        android:orientation="vertical"
                        android:paddingLeft="20dp" >

                        <TextView
                            android:id="@+id/txt_privacy_blocked_user"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:gravity="left|center"
                            android:text="@string/blocked_user_en"
                            android:textColor="@color/maingrey"
                            android:textSize="@dimen/normal_font" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight=".2"
                        android:layoutDirection="ltr"
                        android:orientation="horizontal"
                        android:paddingLeft="20dp"
                        android:weightSum="1" >

                        <TextView
                            android:id="@+id/txt_privacy_last_seen"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight=".5"
                            android:gravity="left|center"
                            android:text="@string/lastseen_en"
                            android:textColor="@color/maingrey"
                            android:textSize="@dimen/normal_font" />

                        <TextView
                            android:id="@+id/txt_privacy_last_seen_selection"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight=".5"
                            android:gravity="right|center"
                            android:paddingRight="@dimen/dp20"
                            android:text="@string/everybody_en"
                            android:textColor="@color/light_green"
                            android:textSize="@dimen/normal_font" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight=".2"
                        android:layoutDirection="ltr"
                        android:orientation="vertical"
                        android:paddingLeft="20dp" >

                        <TextView
                            android:id="@+id/txt_privacy_who_can_see"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:gravity="left|center"
                            android:paddingRight="@dimen/dp20"
                            android:singleLine="true"
                            android:text="@string/who_can_seeyou_en"
                            android:textColor="@color/orang"
                            android:textSize="@dimen/normal_font" />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

            <View
                android:id="@+id/view1"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/orang" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp200"
                android:layoutDirection="ltr"
                android:weightSum="10" >

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:layoutDirection="ltr"
                    android:orientation="vertical" >
                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="9"
                    android:layoutDirection="ltr"
                    android:orientation="vertical"
                    android:weightSum="1.3" >

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight=".4"
                        android:layoutDirection="ltr"
                        android:orientation="vertical" >

                        <TextView
                            android:id="@+id/txt_str_security"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:gravity="left|center"
                            android:paddingTop="5dp"
                            android:singleLine="true"
                            android:text="@string/security_en"
                            android:textColor="@color/light_green"
                            android:textSize="@dimen/xlarg_font" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight=".2"
                        android:layoutDirection="ltr"
                        android:orientation="vertical"
                        android:paddingLeft="20dp" >

                        <TextView
                            android:id="@+id/txt_privacy_passcode"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:gravity="left|center"
                            android:text="@string/passcode_lock_en"
                            android:textColor="@color/maingrey"
                            android:textSize="@dimen/normal_font" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight=".2"
                        android:layoutDirection="ltr"
                        android:orientation="vertical"
                        android:paddingLeft="20dp" >

                        <TextView
                            android:id="@+id/txt_privacy_tow_step_verification"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:gravity="left|center"
                            android:text="@string/tow_step_verification_en"
                            android:textColor="@color/maingrey"
                            android:textSize="@dimen/normal_font" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight=".2"
                        android:layoutDirection="ltr"
                        android:orientation="vertical"
                        android:paddingLeft="20dp" >

                        <TextView
                            android:id="@+id/txt_privacy_active_sessions"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:gravity="left|center"
                            android:text="@string/active_sessions_en"
                            android:textColor="@color/maingrey"
                            android:textSize="@dimen/normal_font" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight=".2"
                        android:layoutDirection="ltr"
                        android:orientation="vertical"
                        android:paddingLeft="20dp" >

                        <TextView
                            android:id="@+id/txt_privacy_control_session"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:gravity="left|center"
                            android:paddingRight="@dimen/dp20"
                            android:singleLine="true"
                            android:text="@string/control_your_session_on_other_devices._en"
                            android:textColor="@color/orang"
                            android:textSize="@dimen/pargraph_font" />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

            <View
                android:id="@+id/view2"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/orang" />
            
            <!-- Start -->
            
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp180"
                android:layoutDirection="ltr"
                android:weightSum="10" >

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:layoutDirection="ltr"
                    android:orientation="vertical" >
                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="9"
                    android:layoutDirection="ltr"
                    android:orientation="vertical"
                    android:weightSum="1.2" >

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight=".5"
                        android:layoutDirection="ltr"
                        android:orientation="vertical" >

                        <TextView
                            android:id="@+id/txt_delete"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:gravity="left|center"
                            android:paddingTop="5dp"
                            android:singleLine="true"
                            android:text="@string/account_delete"
                            android:textColor="@color/light_green"
                            android:textSize="@dimen/xlarg_font" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight=".2"
                        android:layoutDirection="ltr"
                        android:orientation="vertical"
                        android:paddingLeft="20dp" >

                        <TextView
                            android:id="@+id/txt_delete_account"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:gravity="left|center"
                            android:text="@string/account_delete_user"
                            android:textColor="@color/maingrey"
                            android:textSize="@dimen/normal_font" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight=".2"
                        android:layoutDirection="ltr"
                        android:orientation="vertical"
                        android:paddingLeft="20dp" >

                        <TextView
                            android:id="@+id/txt_delete_desc"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:gravity="left|center"
                            android:paddingRight="@dimen/dp20"
                            android:singleLine="true"
                            android:text="@string/description_for_delete_account_en"
                            android:textColor="@color/orang"
                            android:textSize="@dimen/normal_font" />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

            <View
                android:id="@+id/view3"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/orang" />
            
            <!-- End -->

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp250"
                android:layoutDirection="ltr"
                android:weightSum="10" >

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:layoutDirection="ltr"
                    android:orientation="vertical" >
                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="9"
                    android:layoutDirection="ltr"
                    android:orientation="vertical"
                    android:weightSum="1.7" >

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight=".4"
                        android:layoutDirection="ltr"
                        android:orientation="vertical" >

                        <TextView
                            android:id="@+id/txt_str_account_self"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:gravity="left|center"
                            android:paddingTop="5dp"
                            android:singleLine="true"
                            android:text="@string/account_self_destructs_en"
                            android:textColor="@color/light_green"
                            android:textSize="@dimen/xlarg_font" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight=".25"
                        android:layoutDirection="ltr"
                        android:orientation="horizontal"
                        android:paddingLeft="20dp"
                        android:weightSum="1" >

                        <TextView
                            android:id="@+id/txt_str_away"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight=".6"
                            android:gravity="left|center"
                            android:text="@string/ifyou_away_for_en"
                            android:textColor="@color/maingrey"
                            android:textSize="@dimen/normal_font" />

                        <TextView
                            android:id="@+id/txt_away_time"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight=".4"
                            android:gravity="right|center"
                            android:paddingRight="@dimen/dp20"
                            android:text="@string/six_months_en"
                            android:textColor="@color/light_green"
                            android:textSize="@dimen/normal_font" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight=".7"
                        android:gravity="left"
                        android:layoutDirection="ltr"
                        android:orientation="horizontal"
                        android:paddingLeft="20dp" >

                        <TextView
                            android:id="@+id/txt_str_note"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:gravity="left"
                            android:paddingRight="@dimen/dp20"
                            android:paddingTop="@dimen/small_font"
                            android:text="@string/account_delete_not_en"
                            android:textColor="@color/orang"
                            android:textSize="@dimen/normal_font" />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </ScrollView>

</LinearLayout>